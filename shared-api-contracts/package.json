{
  "name": "@calculator/shared-api-contracts",
  "version": "1.0.0",
  "description": "Shared API contracts (DTOs, types, schemas) for the Calculator application ecosystem.",
  "main": "./dist/shared/index.js",
  "types": "./dist/shared/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist coverage",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "api",
    "contracts",
    "dto",
    "typescript",
    "zod"
  ],
  "author": "Enterprise Code Generation Specialist",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/shared-api-contracts.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/shared/index.js",
      "require": "./dist/shared/index.js",
      "types": "./dist/shared/index.d.ts"
    },
    "./auth": {
      "import": "./dist/domains/auth/index.js",
      "require": "./dist/domains/auth/index.js",
      "types": "./dist/domains/auth/index.d.ts"
    },
    "./user": {
      "import": "./dist/domains/user/index.js",
      "require": "./dist/domains/user/index.js",
      "types": "./dist/domains/user/index.d.ts"
    },
    "./custom-mode": {
      "import": "./dist/domains/custom-mode/index.js",
      "require": "./dist/domains/custom-mode/index.js",
      "types": "./dist/domains/custom-mode/index.d.ts"
    },
    "./variable": {
      "import": "./dist/domains/variable/index.js",
      "require": "./dist/domains/variable/index.js",
      "types": "./dist/domains/variable/index.d.ts"
    },
    "./history": {
      "import": "./dist/domains/history/index.js",
      "require": "./dist/domains/history/index.js",
      "types": "./dist/domains/history/index.d.ts"
    }
  },
  "dependencies": {
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}